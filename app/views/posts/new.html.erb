<% provide :title, "Create post" %>
<div class="head">
  <h1>Create your posts</h1>
</div>
<div class="body">
  <%= form_for @post do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :content %>
    <%= f.text_field :content %>
    <%= f.submit :create %>
  <% end %>
</div>
